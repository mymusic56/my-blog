<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="simple" targetRuntime="MyBatis3">
        <commentGenerator>
            <!--去掉日期注释-->
            <!--<property name="suppressDate" value="true"/>-->
            <!--去掉所有注释-->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://home.mytest.com:3306/myblog"
                        userId="root"
                        password="123456">
            <!--<property name="nullCatalogMeansCurrent" value="true" />-->
        </jdbcConnection>
        <javaModelGenerator targetPackage="com.zsj.blog.common.persistence.mybatis.auto.entity"
                            targetProject="blog-common/src/main/java">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mybatis.mapper.auto"
                         targetProject="blog-common/src/main/resources"/>

        <javaClientGenerator type="XMLMAPPER" targetPackage="com.zsj.blog.common.persistence.mybatis.auto.mapper"
                             targetProject="blog-common/src/main/java"/>

        <table tableName="sys_user" domainObjectName="SysUserEntity"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        >
            <generatedKey column="id" sqlStatement="SELECT LAST_INSERT_ID()" identity="true" />
        </table>
        <table tableName="sys_role" domainObjectName="SysRoleEntity"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        >
            <generatedKey column="id" sqlStatement="SELECT LAST_INSERT_ID()" identity="true" />
        </table>
        <table tableName="sys_user_role" domainObjectName="SysUserRoleEntity"
               enableCountByExample="false"
               enableUpdateByExample="false"
               enableDeleteByExample="false"
               enableSelectByExample="false"
               selectByExampleQueryId="false"
        >
            <generatedKey column="id" sqlStatement="SELECT LAST_INSERT_ID()" identity="true" />
        </table>
    </context>
</generatorConfiguration>